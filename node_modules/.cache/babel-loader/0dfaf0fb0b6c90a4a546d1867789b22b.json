{"ast":null,"code":"export default {\n  props: ['parentData'],\n  // emits: [\"my-event\"],\n  data: () => ({\n    listConsultants: [],\n    addD: []\n  }),\n  // created () {\n  //   this.report()\n  // },\n  methods: {\n    onEnlargeText: function (enlargeAmount) {\n      // this.addD = payload\n      console.log('compornent 2 ', enlargeAmount);\n    },\n\n    async report() {\n      const itemConsultants = this.parentData.map(v => {\n        return v.co_usuario;\n      });\n      const request = {\n        item_consultants: itemConsultants\n      };\n\n      try {\n        const response = await this.$store.dispatch(\"consultant/CONSULTANT_SHOW\", request); //response service\n\n        const dataConsultan = response.data;\n        const dataOrganizeConsultant = dataConsultan.reduce(function (r, consultant) {\n          r[consultant.co_usuario] = r[consultant.co_usuario] || [];\n          r[consultant.co_usuario].push(consultant);\n          return r;\n        }, Object.create(null));\n        this.listConsultants = dataOrganizeConsultant;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    finalOperation(arrayObjectcs, key, type = true) {\n      if (!Array.isArray(arrayObjectcs)) {\n        return null;\n      }\n\n      let j = 1;\n      let total = arrayObjectcs[0][key];\n\n      if (!total) {\n        return 0;\n      }\n\n      while (j < arrayObjectcs.length) {\n        if (type) {\n          total += arrayObjectcs[j][key];\n        } else {\n          total -= arrayObjectcs[j][key];\n        }\n\n        j++;\n      }\n\n      return this.numFormat(total);\n    },\n\n    numFormat(item) {\n      return new Intl.NumberFormat('de-DE').format(item.toFixed(2));\n    },\n\n    nameConsultant(item) {\n      const name = this.parentData.find(consultant => consultant.co_usuario === item ? consultant.no_usuario : null);\n      return name.no_usuario;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8DA;EACAA,qBADA;EAEA;EAEAC;IACAC,mBADA;IAEAC;EAFA,EAJA;EAQA;EACA;EACA;EACAC;IACAC;MACA;MACAC;IACA,CAJA;;IAKA;MACA;QACA;MACA,CAFA;MAGA;QACAC;MADA;;MAGA;QACA,mFADA,CACA;;QAEA;QAEA;UACAC;UACAA;UACA;QACA,CAJA,EAIAC,mBAJA;QAMA;MAEA,CAbA,CAaA;QACAH;MACA;IACA,CA5BA;;IA6BAI;MAEA;QACA;MACA;;MAEA;MACA;;MAEA;QACA;MACA;;MAEA;QACA;UACAC;QACA,CAFA,MAGA;UACAA;QACA;;QACAC;MACA;;MACA;IACA,CApDA;;IAqDAC;MACA;IACA,CAvDA;;IAwDAC;MACA;MACA;IACA;;EA3DA;AAXA","names":["props","data","listConsultants","addD","methods","onEnlargeText","console","item_consultants","r","Object","finalOperation","total","j","numFormat","nameConsultant"],"sourceRoot":"src/components","sources":["PerformanceConsultants.vue"],"sourcesContent":["<template>\r\n    <div>\r\n    <v-simple-table \r\n        class=\"pt-3\"\r\n        dense \r\n        v-for=\"(item, i) in listConsultants\"\r\n        :key=\"i\"\r\n        >\r\n            <template v-slot:default>\r\n                <thead class=\"border border-secondary\">\r\n                <tr>\r\n                    <th class=\"text-left\">\r\n                    {{ nameConsultant(item[0].co_usuario) }}\r\n                    </th>\r\n                </tr>\r\n                <tr>\r\n                    <th class=\"text-left border border-secondary\">\r\n                    Período\r\n                    </th>\r\n                    <th class=\"text-center border border-secondary\">\r\n                    Ganancias Netas\r\n                    </th>\r\n                    <th class=\"text-center border border-secondary\">\r\n                    Costo Fijo\r\n                    </th>\r\n                    <th class=\"text-center border border-secondary\">\r\n                    Comisión\r\n                    </th>\r\n                    <th class=\"text-center border border-secondary\">\r\n                    Beneficio\r\n                    </th>\r\n                </tr>\r\n                </thead>\r\n                <tbody\r\n                \r\n                >\r\n                <tr\r\n                v-for=\"(lis, i) in item\"\r\n                :key=\"i\"\r\n                class=\"border border-secondary\"\r\n                >\r\n                    <td class=\"border border-secondary\">{{ lis.month }}</td>\r\n                    <td class=\"border border-secondary\">R$ {{ numFormat(lis.total_net_earnings)}}</td>\r\n                    <td class=\"border border-secondary\">R$ {{ numFormat(lis.fixed_cost) }}</td>\r\n                    <td class=\"border border-secondary\">R$ {{ numFormat(lis.total_commission) }}</td>\r\n                    <td class=\"border border-secondary\">R$ {{ numFormat(lis.total_lucro) }}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"border border-secondary\">SALDO</td>\r\n                    <td class=\"border border-secondary\">R$ {{ finalOperation(item,'total_net_earnings') }}</td>\r\n                    <td class=\"border border-secondary\">R$ {{ finalOperation(item,'fixed_cost') }}</td>\r\n                    <td class=\"border border-secondary\">R$ {{ finalOperation(item,'total_commission') }}</td>\r\n                    <td class=\"border border-secondary\">R$ {{ finalOperation(item,'total_lucro',false) }}</td>\r\n                </tr>\r\n                </tbody>\r\n            </template>\r\n        </v-simple-table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  props: ['parentData'],\r\n  // emits: [\"my-event\"],\r\n  \r\n  data: () => ({\r\n    listConsultants : [],\r\n    addD : [],\r\n  }),\r\n  // created () {\r\n  //   this.report()\r\n  // },\r\n  methods: {\r\n    onEnlargeText: function (enlargeAmount) {\r\n      // this.addD = payload\r\n      console.log('compornent 2 ',enlargeAmount)\r\n    },\r\n    async report(){\r\n      const itemConsultants = this.parentData.map((v) => {\r\n          return v.co_usuario\r\n      });\r\n      const request = {\r\n        item_consultants: itemConsultants\r\n      };\r\n      try {\r\n        const response = await this.$store.dispatch(\"consultant/CONSULTANT_SHOW\", request);//response service\r\n        \r\n        const dataConsultan = response.data;\r\n\r\n        const dataOrganizeConsultant = dataConsultan.reduce(function (r, consultant){\r\n          r[consultant.co_usuario] = r[consultant.co_usuario] || [];\r\n          r[consultant.co_usuario].push(consultant); \r\n          return r;\r\n        }, Object.create(null));\r\n\r\n        this.listConsultants = dataOrganizeConsultant;\r\n\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    finalOperation(arrayObjectcs, key, type = true) {\r\n\r\n      if(!Array.isArray(arrayObjectcs)){\r\n        return null;\r\n      }\r\n\r\n      let j = 1;\r\n      let total = arrayObjectcs[0][key];\r\n\r\n      if(!total){\r\n        return 0;\r\n      }\r\n\r\n      while (j< arrayObjectcs.length) {\r\n        if(type){\r\n          total += arrayObjectcs[j][key];\r\n        }\r\n        else{\r\n          total -= arrayObjectcs[j][key];\r\n        }\r\n        j++;\r\n      }\r\n      return this.numFormat(total);\r\n    },\r\n    numFormat(item) {\r\n      return new Intl.NumberFormat('de-DE').format(item.toFixed(2));\r\n    },\r\n    nameConsultant(item) {\r\n      const name = this.parentData.find(consultant => consultant.co_usuario === item ? consultant.no_usuario: null);\r\n      return name.no_usuario\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n  tbody {\r\n      tr:hover{\r\n        background-color: transparent !important;\r\n      }\r\n  }\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}