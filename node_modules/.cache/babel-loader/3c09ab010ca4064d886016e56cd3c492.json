{"ast":null,"code":"import Swal from 'sweetalert2';\nimport { sortArrayItems } from '../assets/util/util'; // import { test } from '../assets/util/util';\n\nimport PerformanceComponent from './PerformanceConsultants';\nimport GraphBarComponent from './GraphBar';\nimport GraphPizzaComponent from './GraphPizza';\nexport default {\n  components: {\n    PerformanceComponent,\n    GraphBarComponent,\n    GraphPizzaComponent\n  },\n  data: () => ({\n    dataConsultan: [],\n    itemBoxA: [],\n    ItemsTotalSelect: [],\n    itemBoxB: [],\n    currentTabComponent: false\n  }),\n  computed: {\n    ItemsConsultants() {\n      return this.$store.getters[\"consultant/GET_CONSULTANTS\"];\n    }\n\n  },\n\n  created() {\n    this.initialize();\n  },\n\n  mounted() {\n    sortArrayItems(); // test();\n  },\n\n  methods: {\n    async initialize() {\n      try {\n        const response = await this.$store.dispatch(\"consultant/INDEX_CONSULTANTS\");\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    addConsultant() {\n      try {\n        const sizeSelect = this.itemBoxA.length;\n\n        if (sizeSelect > 0) {\n          for (let index = 0; index < this.itemBoxA.length; index++) {\n            const position = this.ItemsConsultants.findIndex(element => {\n              return element.co_usuario === this.itemBoxA[index];\n            });\n\n            if (position !== undefined) {\n              this.ItemsTotalSelect.push(this.ItemsConsultants[position]);\n              this.ItemsConsultants.splice(position, 1);\n              sortArrayItems(this.ItemsTotalSelect);\n            }\n          } // test(this.itemBoxA,this.ItemsConsultants,this.ItemsTotalSelect);\n\n        } else {\n          Swal.fire({\n            icon: 'error',\n            title: 'Error',\n            text: 'Seleccione al menos un consultor del listado.'\n          });\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    removeConsultant() {\n      try {\n        const sizeSelect = this.itemBoxB.length;\n\n        if (sizeSelect > 0) {\n          for (let index = 0; index < this.itemBoxB.length; index++) {\n            const position = this.ItemsTotalSelect.findIndex(element => {\n              return element.co_usuario === this.itemBoxB[index];\n            });\n\n            if (position !== undefined) {\n              this.ItemsConsultants.push(this.ItemsTotalSelect[position]);\n              this.ItemsTotalSelect.splice(position, 1);\n              sortArrayItems(this.ItemsConsultants);\n            }\n          }\n        } else {\n          Swal.fire({\n            icon: 'error',\n            title: 'Error',\n            text: 'Seleccione al menos un consultor del listado.'\n          });\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    switchComponent(nameComponent) {\n      try {\n        this.currentTabComponent = nameComponent;\n        this.dataConsultan = this.ItemsTotalSelect;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    sortArrayItems // test\n\n  }\n};","map":{"version":3,"mappings":"AA+HA;AAEA,qD,CACA;;AACA;AACA;AACA;AAEA;EACAA;IACAC,oBADA;IAEAC,iBAFA;IAGAC;EAHA,CADA;EAMAC;IACAC,iBADA;IAEAC,YAFA;IAGAC,oBAHA;IAIAC,YAJA;IAKAC;EALA,EANA;EAaAC;IACAC;MACA;IACA;;EAHA,CAbA;;EAkBAC;IACA;EACA,CApBA;;EAqBAC;IACAC,iBADA,CAEA;EACA,CAxBA;;EAyBAC;IACA;MACA;QACA;MACA,CAFA,CAEA;QACAC;MACA;IACA,CAPA;;IAQAC;MACA;QACA;;QACA;UACA;YACA;cACA;YACA,CAFA;;YAGA;cACA;cACA;cACAH;YACA;UACA,CAVA,CAWA;;QACA,CAZA,MAYA;UACAI;YACAC,aADA;YAEAC,cAFA;YAGAC;UAHA;QAKA;MAEA,CAtBA,CAsBA;QACAL;MACA;IACA,CAlCA;;IAmCAM;MACA;QACA;;QACA;UACA;YACA;cACA;YACA,CAFA;;YAGA;cACA;cACA;cACAR;YACA;UACA;QACA,CAXA,MAWA;UACAI;YACAC,aADA;YAEAC,cAFA;YAGAC;UAHA;QAKA;MACA,CApBA,CAoBA;QACAL;MACA;IACA,CA3DA;;IA4DAO;MACA;QACA;QACA;MACA,CAHA,CAGA;QACAP;MACA;IACA,CAnEA;;IAoEAF,cApEA,CAqEA;;EArEA;AAzBA","names":["components","PerformanceComponent","GraphBarComponent","GraphPizzaComponent","data","dataConsultan","itemBoxA","ItemsTotalSelect","itemBoxB","currentTabComponent","computed","ItemsConsultants","created","mounted","sortArrayItems","methods","console","addConsultant","Swal","icon","title","text","removeConsultant","switchComponent"],"sourceRoot":"src/components","sources":["Consultants.vue"],"sourcesContent":["<template>\r\n  <v-container \r\n    class=\"justify-content-center grey lighten-5\" fluid\r\n  >\r\n    <v-row\r\n      no-gutters\r\n      class=\"center\"\r\n    >\r\n      <v-col\r\n      cols=\"12\"\r\n      md=\"12\"\r\n      sm=\"12\"\r\n      >\r\n        <v-card\r\n          class=\"border pa-2 border-secondary\"\r\n          outlined\r\n          tile\r\n        >\r\n          <v-simple-table >\r\n            <template v-slot:default>\r\n              <tbody class=\"text-center\">\r\n                <tr>\r\n                  <th colspan=\"1\" class=\"text-left border border-secondary table-active\">Período</th>\r\n                  <th scope=\"col\" class=\"text-center border border-secondary\"></th>\r\n                  <th  scope=\"col\" class=\"text-center border border-secondary\">Acci&oacute;n</th>\r\n                </tr>\r\n                <tr class=\"border border-secondary\">\r\n                  <th scope=\"row\" class=\"table-active\">Consultores</th>\r\n                  <td colspan=\"1\" class=\"border border-secondary\">\r\n                    <v-row\r\n                      no-gutters\r\n                    >\r\n                      <v-col cols=\"12\" md=\"5\" sm=\"12\" class=\"p-2\">\r\n                        <select size=\"6\" style=\"width: 223px;\" class=\"border border-secondary\" multiple v-model=\"itemBoxA\">\r\n                          <option\r\n                            v-for=\"(boxA, i) in ItemsConsultants\"\r\n                            :key=\"i\"\r\n                            :value=\"boxA.co_usuario\">\r\n                            {{ boxA.no_usuario }}\r\n                          </option>\r\n 